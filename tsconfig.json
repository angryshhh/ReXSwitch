{
  "compilerOptions": {
    "module": "esnext",
    "noEmit": true,
    "target": "ES2021",
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "jsx": "react",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
